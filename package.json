{
  "name": "@schweizerischebundesbahnen/journey-maps-client",
  "version": "2.7.0",
  "scripts": {
    "ng": "ng",
    "start-testapp": "npm run testapp-example && ng serve journey-maps-client-testapp",
    "start-testapp-with-remote-access": "npm run testapp-example && ng serve journey-maps-client-testapp --host 0.0.0.0 --disable-host-check",
    "start-elements": "ng serve journey-maps-client-elements --port 4201",
    "start-elements-with-remote-access": "ng serve journey-maps-client-elements --port 4201 --host 0.0.0.0 --disable-host-check",
    "build-lib": "ng build journey-maps-client --configuration production",
    "build-testapp": "npm run testapp-example && ng build journey-maps-client-testapp --configuration production --progress=false",
    "build-elements": "ng build journey-maps-client-elements --configuration production --single-bundle --outputHashing=none",
    "postbuild-elements": "copyfiles -f projects/journey-maps-client-elements/README.md projects/journey-maps-client-elements/package.json dist/journey-maps-client-elements",
    "build-elements-with-stats": "ng build journey-maps-client-elements --configuration production --single-bundle --outputHashing=none --stats-json",
    "build-apps": "concurrently --kill-others-on-fail npm:build-testapp npm:build-elements",
    "lint": "ng lint",
    "test-headless": "concurrently npm:test-lib npm:test-testapp",
    "test-lib": "ng test journey-maps-client --watch=false --browsers=ChromeHeadlessNoSandbox",
    "test-testapp": "ng test journey-maps-client-testapp --watch=false --browsers=ChromeHeadlessNoSandbox",
    "test-elements": "ng test journey-maps-client-elements --watch=false --browsers=ChromeHeadlessNoSandbox",
    "compodoc": "trash dist/documentation && compodoc",
    "pretestapp-example": "trash \"projects/journey-maps-client-testapp/src/assets/example/app.component.*\"",
    "testapp-example": "copyfiles -f \"projects/journey-maps-client-testapp/src/app/app.component.*\" projects/journey-maps-client-testapp/src/assets/example",
    "posttestapp-example": "renamer --force --find '/(app\\.component\\..*)$/' --replace '$1.txt' projects/journey-maps-client-testapp/src/assets/example/*",
    "postinstall": "ngcc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/elements": "^13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/localize": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@sbb-esta/angular-core": "^12.4.0",
    "@sbb-esta/angular-public": "^12.4.0",
    "@types/mapbox-gl": "^1.13.0",
    "angular-resize-event": "^2.1.0",
    "document-register-element": "^1.7.2",
    "lorem-ipsum": "^2.0.3",
    "maplibre-gl": "^1.15.2",
    "ngx-build-plus": "^12.2.0",
    "ngx-highlightjs": "^5.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.8.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "concurrently": "^6.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.3",
    "protractor": "~7.0.0",
    "puppeteer": "^10.4.0",
    "renamer": "^4.0.0",
    "trash-cli": "~4.0.0",
    "ts-node": "~10.2.1",
    "typescript": "~4.5.5",
    "webpack-dev-middleware": "^5.2.1"
  }
}
