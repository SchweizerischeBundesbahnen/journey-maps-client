{
  "name": "@schweizerischebundesbahnen/journey-maps-client",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start-testapp": "npm run _testapp-example && ng serve journey-maps-client-testapp",
    "start-testapp-with-remote-access": "npm run _testapp-example && ng serve journey-maps-client-testapp --host 0.0.0.0 --disable-host-check",
    "start-elements": "ng serve journey-maps-client-elements --port 4201",
    "build-lib": "ng build journey-maps-client --prod",
    "build-lib-watch": "ng build journey-maps-client --watch",
    "build-testapp": "npm run _testapp-example && ng build journey-maps-client-testapp --prod --progress=false",
    "build-elements": "npm run _build-elements && npm run _copy-elements-package",
    "build-elements-with-stats": "ng build journey-maps-client-elements --prod --single-bundle --outputHashing=none --stats-json",
    "lint": "ng lint",
    "test-headless": "concurrently npm:test-lib npm:test-testapp",
    "test-lib": "ng test journey-maps-client --watch=false --browsers=ChromeHeadless",
    "test-testapp": "ng test journey-maps-client-testapp --watch=false --browsers=ChromeHeadless",
    "test-elements": "ng test journey-maps-client-elements --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "compodoc": "trash dist/documentation && compodoc",
    "_build-elements": "ng build journey-maps-client-elements --prod --single-bundle --outputHashing=none",
    "_copy-elements-package": "copyfiles -f projects/journey-maps-client-elements/package.json dist/journey-maps-client-elements",
    "_testapp-example": "npm run _example-clean && npm run _example-copy-source && npm run _example-rename-source",
    "_example-clean": "trash \"projects/journey-maps-client-testapp/src/assets/example/app.component.*\"",
    "_example-copy-source": "copyfiles -f \"projects/journey-maps-client-testapp/src/app/app.component.*\" projects/journey-maps-client-testapp/src/assets/example",
    "_example-rename-source": "renamer --force --find '/(app\\.component\\..*)$/' --replace '$1.txt' projects/journey-maps-client-testapp/src/assets/example/*",
    "postinstall": "ngcc"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/elements": "^11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/localize": "^11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@sbb-esta/angular-core": "^11.1.0",
    "@sbb-esta/angular-public": "^11.1.0",
    "@types/mapbox-gl": "^1.13.0",
    "angular-resize-event": "^2.1.0",
    "document-register-element": "^1.7.2",
    "lorem-ipsum": "^2.0.3",
    "mapbox-gl": "^1.13.0",
    "ngx-build-plus": "^11.0.0",
    "ngx-highlightjs": "^4.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.21",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "puppeteer": "^5.5.0",
    "renamer": "^2.0.1",
    "trash-cli": "~4.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
