{
  "name": "journey-maps-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-example && ng serve",
    "start-with-remote-access": "npm run build-example && ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build-lib": "ng build journey-maps-client --prod",
    "build-lib-watch": "ng build journey-maps-client --watch",
    "build-example": "npm run example-clean && npm run example-copy-source && npm run example-rename-source",
    "build-app": "npm run build-example && ng build journey-maps-client-testapp --prod --progress=false",
    "example-clean": "trash \"projects/journey-maps-client-testapp/src/assets/example/app.component.*\"",
    "example-copy-source": "copyfiles -f \"projects/journey-maps-client-testapp/src/app/app.component.*\" projects/journey-maps-client-testapp/src/assets/example",
    "example-rename-source": "renamer --force --find '/(app\\.component\\..*)$/' --replace '$1.txt' projects/journey-maps-client-testapp/src/assets/example/*",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.1",
    "@angular/cdk": "^10.2.6",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/localize": "^10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/router": "~10.2.1",
    "@sbb-esta/angular-core": "^10.3.2",
    "@sbb-esta/angular-icons": "^10.3.2",
    "@sbb-esta/angular-public": "^10.3.2",
    "@types/mapbox-gl": "^1.12.2",
    "lorem-ipsum": "^2.0.3",
    "mapbox-gl": "^1.12.0",
    "ngx-highlightjs": "^4.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "puppeteer": "^5.3.1",
    "renamer": "^2.0.1",
    "trash-cli": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
